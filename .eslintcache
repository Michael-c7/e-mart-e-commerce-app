r\n        try {\r\n            const { data } = await axios.get(`/api/products?id=${productID}`)\r\n            setProduct(data)\r\n        } catch (error) {\r\n\r\n        }\r\n        setLoading(false)\r\n    }\r\n\r\n    useEffect(() => {\r\n        fetchData()\r\n    }, [])\r\n\r\n\r\n    if(loading) {\r\n        return (\r\n            <section className=\"section section-center\">\r\n                <h2>Loading...</h2>\r\n            </section>\r\n        )\r\n    }\r\n\r\n    // const { fields } = product\r\n    // const { name, description, price, image } = fields\r\n  \r\n    return (\r\n      <section className='section section-center'>\r\n        {/* <Link to='/' className='link'>\r\n          Back Home\r\n        </Link>\r\n        <div>\r\n          <div className='title'>\r\n            <h2>{name}</h2>\r\n            <div className='title-underline'></div>\r\n          </div>\r\n          <article className='single-product'>\r\n            <img className='single-product-img' src={image[0].url} alt={name} />\r\n            <div>\r\n              <h5>{name}</h5>\r\n              <h5 className='price'>${price}</h5>\r\n              <p>{description}</p>\r\n            </div>\r\n          </article>\r\n        </div> */}\r\n        <h2>product page</h2>\r\n      </section>\r\n    )\r\n}\r\n\r\nexport default Product",["89","90"],"C:\\Users\\mcarr\\Desktop\\all-code-files\\react-learning\\current\\e-mart-e-commerce-app\\src\\Airtable.js",["91","92"],"C:\\Users\\mcarr\\Desktop\\all-code-files\\react-learning\\current\\e-mart-e-commerce-app\\src\\pages\\HomePage.js",["93","94","95","96","97"],"C:\\Users\\mcarr\\Desktop\\all-code-files\\react-learning\\current\\e-mart-e-commerce-app\\src\\components\\Navbar.js",["98"],"import React from 'react'\r\nimport styled from 'styled-components'\r\nimport { Link } from 'react-router-dom';\r\nimport { BsSearch, BsBag } from 'react-icons/bs';\r\nimport { GiHamburgerMenu } from \"react-icons/gi\"\r\nimport eMartLogo from \