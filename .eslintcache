p-last {\r\n      position:relative;\r\n      display:flex;\r\n      align-items:center;\r\n    }\r\n\r\n    .navbar__cart-icon:hover,\r\n    .navbar__search:hover {\r\n      color:var(--main-color);\r\n      transition:color 0.2s ease;\r\n      cursor:pointer;\r\n    }\r\n\r\n\r\n    .navbar__cart-container {\r\n      position:relative;\r\n    }\r\n\r\n    .navbar__cart-total {\r\n      position:relative;\r\n      top:-8px;\r\n      font-size:0.95rem;\r\n      font-weight:500;\r\n      margin-left:0.5rem;\r\n    }\r\n\r\n    .navbar__item-amt {\r\n      position:absolute;\r\n      font-size:0.7rem;\r\n      background:#000;\r\n      width:20px;\r\n      height:20px;\r\n      border-radius:100px;\r\n      left:0%;\r\n      top:50%;\r\n      transform:translate(-50%, -0%);\r\n    }\r\n\r\n    .navbar__item-amt span {\r\n      position:absolute;\r\n      left:50%;\r\n      top:50%;\r\n      transform:translate(-50%, -50%);\r\n      color:#fff;\r\n    }\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n    // 4k view \r\n    @media screen and (min-width: 2560px) {\r\n      .navbar__logo {\r\n        transform:scale(1.1);\r\n      }\r\n\r\n      .navbar__item {\r\n        font-size:1.475rem;\r\n      }\r\n\r\n      .navbar__icon {\r\n        font-size:1.8rem;\r\n      }\r\n    }\r\n\r\n\r\n    // mobile view \r\n    @media screen and (max-width: 768px) {\r\n      .navbar__hamburger-menu { \r\n        display:block;\r\n      }\r\n\r\n      .navbar__group__second {\r\n        display:none;\r\n      }\r\n    }\r\n\r\n    \r\n    @media screen and (max-width: 375px) {\r\n      .navbar  {\r\n        display:flex;\r\n        flex-direction:column;\r\n        justify-content:center;\r\n        align-items:center;\r\n      }\r\n    }\r\n\r\n    \r\n`","C:\\Users\\mcarr\\Desktop\\all-code-files\\react-learning\\current\\e-mart-e-commerce-app\\src\\pages\\SingleProductPage.js",[],"C:\\Users\\mcarr\\Desktop\\all-code-files\\react-learning\\current\\e-mart-e-commerce-a